    "@itwin/eslint-plugin": "rc",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@types/node": "14.14.31",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.22",
    "mocha": "^8.2.3",
    "nyc": "^15.1.0",
    "sinon": "^9.0.0",
    "eslint": "^7.32.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.3.5"
